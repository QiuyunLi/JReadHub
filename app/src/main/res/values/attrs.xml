<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="readhubTheme" format="color" />
    <attr name="readhubStatus" format="color"/>
    <attr name="readhubIcon" format="color" />
    <attr name="readhubColorAccent" format="color"/>
    <attr name="readhubBackground" format="color" />
    <attr name="readhubViewBackground" format="color" />
    <attr name="readhubDrawerBackground" format="color" />
    <attr name="readhubFloatBtn" format="color" />
    <attr name="readhubToolbarText" format="color" />

    <declare-styleable name="SettingItemView">
        <attr name="icon" format="reference" />
        <attr name="title" format="string" />
        <attr name="description" format="string" />
        <attr name="type" format="enum">
            <enum name="none" value="0" />
            <enum name="switcher" value="1" />
            <enum name="checkbox" value="2" />
        </attr>
    </declare-styleable>
</resources>
